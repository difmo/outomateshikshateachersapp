<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
   android:background="@drawable/collegebackground"
    android:layout_height="match_parent"
    tools:context=".View.Activity.Call_activity">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/calling"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView

        android:id="@+id/textView"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_marginStart="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:inputType="phone"
        android:text="Teacher Contact Number "
        android:textColor="#000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.053" />

    <TextView
        android:id="@+id/firstnumber"
        android:layout_width="157dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:inputType="phone"
        android:text="09026066086"
        android:textColor="#000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.253" />


    <Button
        android:id="@+id/call_btns"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_marginEnd="72dp"
        android:background="#5CCD61"
        android:text="@string/let_s_call"
        android:textColor="#000"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/firstnumber"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toEndOf="@+id/firstnumber"
        app:layout_constraintTop_toTopOf="@+id/firstnumber" />

    <TextView
        android:id="@+id/second_number2"
        android:layout_width="157dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:gravity="center"
        android:fontFamily="sans-serif-black"
        android:inputType="phone"
        android:text="0451656656"
        android:textColor="#000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.358" />

    <Button
        android:id="@+id/call_btns2"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginEnd="72dp"
        android:background="#5CCD61"
        android:text="@string/let_s_call"
        android:textColor="#000"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/second_number2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toEndOf="@+id/second_number2"
        app:layout_constraintTop_toTopOf="@+id/second_number2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/number_input3"
        android:layout_width="157dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:inputType="phone"
        android:text="0451656656"
        android:textColor="#000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.467" />

    <Button
        android:id="@+id/call_btns3"
        android:layout_width="wrap_content"
        android:layout_height="29dp"
        android:layout_marginEnd="72dp"
        android:background="#5CCD61"
        android:text="@string/let_s_call"
        android:textColor="#000"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/number_input3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.589"
        app:layout_constraintStart_toEndOf="@+id/number_input3"
        app:layout_constraintTop_toTopOf="@+id/number_input3"
        app:layout_constraintVertical_bias="0.0" />

    <include
        layout="@layout/footer_for_all"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>